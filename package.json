  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "golos-js": "^0.7.3",
    "lodash": "^4.17.10",
    "next": "^6.1.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-intl": "^2.4.0",
    "styled-components": "^2.1.0",
    "styled-is": "^1.1.5"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
